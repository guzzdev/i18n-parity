{
  "name": "i18n-parity",
  "version": "0.1.0",
  "description": "Validate parity of translation keys across locales: missing, empty, extra keys and coverage %",
  "type": "module",
  "bin": {
    "i18n-parity": "dist/cli.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/cli.ts --format esm --dts",
    "dev": "tsx src/cli.ts",
    "test": "tsx --test",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "i18n",
    "localization",
    "translations",
    "lint",
    "parity",
    "coverage",
    "cli"
  ],
  "author": "Louis <guzzdev@users.noreply.github.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/guzzdev/i18n-parity.git"
  },
  "bugs": {
    "url": "https://github.com/guzzdev/i18n-parity/issues"
  },
  "homepage": "https://github.com/guzzdev/i18n-parity#readme",
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@types/bun": "^1.3.4",
    "@types/node": "^25.0.2",
    "tsup": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.6.0"
  }
}